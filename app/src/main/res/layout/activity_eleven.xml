<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="elevenVM" type="com.kahuripaninfox.app.modules.eleven.data.viewmodel.ElevenVM"/>
    </data>
    <LinearLayout android:id="@+id/linearEleven"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageButton
            android:background="@drawable/rectangle_bg_white_a700_4c_radius_4"
            android:id="@+id/btnFrame2608590"
            android:layout_gravity="start"
            android:layout_height="@dimen/_40pxh"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="@dimen/_40pxh"
            android:paddingHorizontal="@dimen/_12pxh"
            android:paddingVertical="@dimen/_12pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_frame2608590" tools:ignore="ContentDescription"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumn500px036view"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_2pxh"
            android:layout_marginStart="@dimen/_48pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius">
            <FrameLayout android:id="@+id/frameStack500px036view"
                android:layout_height="@dimen/_138pxv"
                android:layout_width="@dimen/_361pxh" style="@style/groupStylecornerRadius_1">
                <ImageView android:id="@+id/image500px036View"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_138pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_width="@dimen/_248pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_500px036view}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_500px036view"/>
            </FrameLayout>
            <TextView android:ellipsize="end"

                android:id="@+id/txtCandiGunungGa"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:letterSpacing="0.03" android:singleLine="true"
                android:text="@{elevenVM.elevenModel.txtCandiGunungGa}"
                style="@style/txtNobilemedium12" tools:text="@string/msg_candi_gunung_ga"/>

            <LinearLayout
                android:id="@+id/linearListmenu"
                android:layout_width="172dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtDistance"
                    style="@style/txtInterregular12"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.29"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{elevenVM.elevenModel.txtDistance}"
                    tools:text="@string/lbl_1_0km2" />

                <ImageView
                    android:id="@+id/imageSix"
                    android:layout_width="@dimen/_4pxh"
                    android:layout_height="@dimen/_4pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_" />

                <ImageView
                    android:id="@+id/imageStar"
                    android:layout_width="@dimen/_13pxh"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_star" />

                <TextView
                    android:id="@+id/txtReviewsCounter"
                    style="@style/txtInterregular12"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.56"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{elevenVM.elevenModel.txtReviewsCounter}"
                    tools:text="@string/lbl_4_8_reviews" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumn500px036viewOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_6pxh"
            android:layout_marginStart="@dimen/_48pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius">
            <FrameLayout android:id="@+id/frameStack500px036viewOne"
                android:layout_height="@dimen/_138pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="@dimen/_341pxh" style="@style/groupStylecornerRadius_1">
                <ImageView android:id="@+id/image500px036ViewOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_138pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_width="@dimen/_248pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_500px036view_138x248}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_500px036view_138x248"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCandiWatutulis"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:letterSpacing="0.03" android:singleLine="true"
                android:text="@{elevenVM.elevenModel.txtCandiWatutulis}"
                style="@style/txtNobilemedium12" tools:text="@string/lbl_candi_watutulis"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearListmenu1"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_365pxv"
            android:layout_marginEnd="@dimen/_48pxh"
            android:layout_marginStart="@dimen/_48pxh"
            android:layout_marginTop="@dimen/_2pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtDistanceOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.3" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{elevenVM.elevenModel.txtDistanceOne}"
                style="@style/txtInterregular12" tools:text="@string/lbl_500_m"/>
            <ImageView android:id="@+id/imageSeven"
                android:layout_height="@dimen/_4pxh"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="@dimen/_4pxh"
                android:scaleType="fitXY" android:src="@drawable/img_"
                tools:ignore="ContentDescription" tools:src="@drawable/img_"/>
            <ImageView android:id="@+id/imageStarOne"
                android:layout_height="@dimen/_12pxh"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_13pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_star"
                tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtReviewsCounterOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_9pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.55" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{elevenVM.elevenModel.txtReviewsCounterOne}"
                style="@style/txtInterregular12" tools:text="@string/lbl_4_8_reviews"/>
        </LinearLayout>
    </LinearLayout>
</layout>
