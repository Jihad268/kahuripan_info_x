<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="twentyVM" type="com.kahuripaninfox.app.modules.twenty.data.viewmodel.TwentyVM"/>
    </data>
    <LinearLayout android:id="@+id/linearTwenty"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_688pxv"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_weight="0.89" android:layout_width="0dp" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearColumnvector"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal" style="@style/groupStylewhite_A700_4ccornerRadius">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="@dimen/_7pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_frame2608590"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_frame2608590"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBahasa"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_30pxh"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{twentyVM.twentyModel.txtBahasa}"
                        style="@style/txtPoppinssemibold16" tools:text="@string/lbl_bahasa"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowflagofindones"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFlagofIndones"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_37pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_flagofindones"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_flagofindones"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCountry"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_weight="0.76"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{twentyVM.twentyModel.txtCountry}"
                        style="@style/txtPoppinsmedium13" tools:text="@string/msg_bahasa_indonesi"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowflagofrussia"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFlagofRussia"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_width="@dimen/_33pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_flagofrussia"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_flagofrussia"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFive"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.57"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{twentyVM.twentyModel.txtFive}"
                        style="@style/txtPoppinsmedium13" tools:text="@string/lbl3"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ImageView android:id="@+id/imagePreviewremoveb"
            android:layout_height="@dimen/_27pxh"
            android:layout_marginBottom="@dimen/_723pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_169pxh"
            android:layout_marginTop="@dimen/_73pxv"
            android:layout_width="@dimen/_25pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_previewremoveb"
            tools:ignore="ContentDescription" tools:src="@drawable/img_previewremoveb"/>
    </LinearLayout>
</layout>
