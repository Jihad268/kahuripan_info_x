<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="sixteenVM" type="com.kahuripaninfox.app.modules.sixteen.data.viewmodel.SixteenVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearSixteen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowframe2608590"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_white_a700_4c_radius_4"
                    android:id="@+id/btnFrame2608590"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:paddingHorizontal="@dimen/_12pxh"
                    android:paddingVertical="@dimen/_12pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_frame2608590" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNotifikasi"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_weight="0.66"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{sixteenVM.sixteenModel.txtNotifikasi}"
                    style="@style/txtPoppinssemibold16" tools:text="@string/lbl_notifikasi"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <View android:background="@color/bluegray_100"
            android:id="@+id/lineLineThree"
            android:layout_height="@dimen/_2pxv"
            android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_411pxh"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnkomentar"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_600pxv"
            android:layout_marginEnd="@dimen/_7pxh"
            android:layout_marginStart="@dimen/_7pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtKomentar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_38pxh"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{sixteenVM.sixteenModel.txtKomentar}"
                style="@style/txtPoppinsregular16" tools:text="@string/lbl_komentar"/>
            <LinearLayout android:id="@+id/linearRowunsplashjmurdh"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageUnsplashjmURdh"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_width="@dimen/_35pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_unsplashjmurdh}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_unsplashjmurdh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPenggunaBaru"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="0.27"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{sixteenVM.sixteenModel.txtPenggunaBaru}"
                    style="@style/txtPoppinssemibold12" tools:text="@string/lbl_pengguna_baru"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMembalasChatA"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_weight="0.37"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{sixteenVM.sixteenModel.txtMembalasChatA}"
                    style="@style/txtPoppinsregular12_1" tools:text="@string/msg_membalas_chat_a"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJamCounter"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_weight="0.09"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{sixteenVM.sixteenModel.txtJamCounter}"
                    style="@style/txtPoppinsregular12_2" tools:text="@string/lbl_1_jam"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtButton"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_weight="0.18"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:paddingEnd="@dimen/_12pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:singleLine="true"
                    android:text="@{sixteenVM.sixteenModel.txtButton}"
                    style="@style/txtSolidRoundedOutline_1" tools:text="@string/lbl_lihat"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtInformasi"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{sixteenVM.sixteenModel.txtInformasi}"
                style="@style/txtPoppinsregular16" tools:text="@string/lbl_informasi"/>
            <LinearLayout android:id="@+id/linearRow"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowfour"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.55"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFour"
                        android:layout_height="@dimen/_23pxh"
                        android:layout_width="@dimen/_23pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img__23x23"
                        tools:ignore="ContentDescription" tools:src="@drawable/img__23x23"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAdaInfomasiBa"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{sixteenVM.sixteenModel.txtAdaInfomasiBa}"
                        style="@style/txtPoppinsregular12_1" tools:text="@string/msg_ada_infomasi_ba"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowmenitcounter"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_92pxh"
                    android:layout_weight="0.45"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMenitCounter"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.4"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{sixteenVM.sixteenModel.txtMenitCounter}"
                        style="@style/txtPoppinsregular12_2" tools:text="@string/lbl_1_menit"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtButtonOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_weight="0.6"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:paddingEnd="@dimen/_12pxh"
                        android:paddingStart="@dimen/_12pxh"
                        android:singleLine="true"
                        android:text="@{sixteenVM.sixteenModel.txtButtonOne}"
                        style="@style/txtSolidRoundedOutline_1" tools:text="@string/lbl_lihat"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
