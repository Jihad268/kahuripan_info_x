<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="eighteenVM" type="com.kahuripaninfox.app.modules.eighteen.data.viewmodel.EighteenVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearEighteen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowvector"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700_4ccornerRadius">
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_7pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_frame2608590"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_frame2608590"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBALASAN"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{eighteenVM.eighteenModel.txtBALASAN}"
                    style="@style/txtPoppinssemibold16" tools:text="@string/lbl_balasan"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout android:id="@+id/frameStackrectangle204One"
            android:layout_gravity="end"
            android:layout_height="@dimen/_174pxv"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_54pxh"
            android:layout_marginTop="@dimen/_34pxv" android:layout_width="@dimen/_349pxh">
            <LinearLayout android:id="@+id/linearComment"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearColumnrectangle204"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.99"
                    android:layout_width="0dp" android:orientation="vertical">
                    <FrameLayout
                        android:id="@+id/frameStackrectangle204"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_70pxv" android:layout_width="@dimen/_327pxh">
                        <LinearLayout
                            android:id="@+id/linearRowrectangle204"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageRectangle204"
                                android:layout_height="@dimen/_24pxh"
                                android:layout_marginBottom="@dimen/_30pxv"
                                android:layout_width="@dimen/_24pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_rectangle204}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle204"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumnkadharefinley"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_17pxv"
                                android:layout_marginEnd="@dimen/_120pxh"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.88"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtKaDhareFinley"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{eighteenVM.eighteenModel.txtKaDhareFinley}"
                                    style="@style/txtRobotoromanregular12" tools:text="@string/msg_kh_xperience"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtTheStrugglewa"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginTop="@dimen/_4pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{eighteenVM.eighteenModel.txtTheStrugglewa}"
                                    style="@style/txtRobotoromanregular14" tools:text="@string/msg_aplikasi_yang_b"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowcomment"
                            android:layout_gravity="bottom|end"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_124pxh"
                            android:layout_marginStart="@dimen/_124pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageComment"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_2pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_10pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_comment"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_comment"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTwoHundred"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.66"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{eighteenVM.eighteenModel.txtTwoHundred}"
                                style="@style/txtRobotoromanregular12_1" tools:text="@string/lbl_200"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowlike"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_34pxh"
                            android:layout_marginStart="@dimen/_34pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageLike"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_13pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_like"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_like"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txt1K"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.36"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{eighteenVM.eighteenModel.txt1K}"
                                style="@style/txtRobotoromanregular12_1" tools:text="@string/lbl_1k"/>
                            <ImageView android:id="@+id/imageDislike"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginEnd="@dimen/_1pxh"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_13pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_like"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_like"/>
                        </LinearLayout>
                    </FrameLayout>
                    <ImageView android:id="@+id/imageRectangle204One"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginEnd="@dimen/_23pxh"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_36pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_rectangle204}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle204"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageMore"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_113pxv"
                    android:layout_marginEnd="@dimen/_7pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_2pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_more"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_more"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearColumnpendatangbaru"
                android:layout_width="285dp"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|end"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtPendatangBaru"
                    style="@style/txtRobotoromanregular12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{eighteenVM.eighteenModel.txtPendatangBaru}"
                    tools:text="@string/msg_pendatang_baru" />

                <TextView
                    android:id="@+id/txtSetujuBanget"
                    style="@style/txtRobotoromanregular14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{eighteenVM.eighteenModel.txtSetujuBanget}"
                    tools:text="@string/lbl_setuju_banget" />

                <LinearLayout
                    android:id="@+id/linearRowlikeOne"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageLikeOne"
                        android:layout_width="@dimen/_13pxh"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_like_black_900"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_like_black_900" />

                    <TextView
                        android:id="@+id/txtSevenHundred"
                        style="@style/txtRobotoromanregular12_2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.44"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{eighteenVM.eighteenModel.txtSevenHundred}"
                        tools:text="@string/lbl_700" />

                    <ImageView
                        android:id="@+id/imageDislikeOne"
                        android:layout_width="@dimen/_13pxh"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_like_black_900"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_like_black_900" />
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameStackunsplashjmurdh"
            android:layout_height="@dimen/_42pxv"
            android:layout_marginBottom="@dimen/_30pxv"
            android:layout_marginTop="@dimen/_483pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageUnsplashjmURdh"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_38pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_width="@dimen/_36pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_unsplashjmurdh"
                tools:ignore="ContentDescription" tools:src="@drawable/img_unsplashjmurdh"/>
            <TextView android:ellipsize="end"
                android:gravity="top|start"
                android:id="@+id/txtBalaslahSesuat"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_62pxh"
                android:layout_marginStart="@dimen/_62pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{eighteenVM.eighteenModel.txtBalaslahSesuat}"
                style="@style/txtPoppinssemibold14_2" tools:text="@string/msg_balaslah_sesuat"/>
            <View
                android:background="@drawable/rectangle_border_black_900"
                android:id="@+id/viewRectangle206"
                android:layout_gravity="start"
                android:layout_height="@dimen/_41pxv"
                android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_411pxh"/>
        </FrameLayout>
    </LinearLayout>
</layout>
